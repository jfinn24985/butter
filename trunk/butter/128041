, \"OPENMP:=-fopenmp\\n\"
, \"\\n\"
, \"endif\\n\"
, \"# END ONCE-ONLY\\n\"
, \"\\n\"
, \"define _makedep_\\n\"
, \"%$(sufdep): %$(1)\\n\"
, \"\\tset -e; $$(CCC) -MM $$(CPPFLAGS) $$< \\\\\\n\"
, \"\\t| sed 's/\\\\($$*\\\\)$$(sufobj)[ :]*/\\\\1$$(sufobj) $$@ : /g' > $$@;\\\\\\n\"
, \"\\t[ -s $$@ ] || rm -f $$@\\n\"
, \"endef\\n\"
, \"\\n\"
, \"$(foreach suff,.c .C .cc .cpp .cxx,$(eval $(call _makedep_,$(suff))))\\n\"
