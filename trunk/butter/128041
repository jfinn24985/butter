, \"endef\\n\"
, \"define do_link_shr\\n\"
, \"$(1) : CCFLAGS+=$(SHRFLAGS)\\n\"
, \"$(1) : CCCFLAGS+=$(SHRFLAGS)\\n\"
, \"$(1) : $(2); $$(LINK.cpp) $$(OUTPUT_OPTIONS) -shared -Wl,-soname,$(1) -o $(1) -Wl,--start-group $(2) -Wl,--end-group\\n\"
, \"endef\\n\"
, \"\\n\"
, \"###############\\n\"
, \"## Editor sugar\\n\"
, \"###############\\n\"
, \"# Local Variables:\\n\"
, \"# mode: makefile\\n\"
, \"# End:\\n\"
, 0 }"
