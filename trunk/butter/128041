, \"CCCOMP = -Wall -pedantic $(CCSTD) $(CCCPP) ;\\n\"
, \"C++COMP = -Wall -Weffc++ $(C++STD) $(C++CPP) ;\\n\"
, \"FORTRANCOMP = -Wall -c ;\\n\"
, \"LINKCOMP = ;\\n\"
, \"OPENMP = -fopenmp ;\\n\"
, \"FORTRANLIBS = -lgfortran ;\\n\"
, \"}\\n\"
, \"\\n################################################\\n\"
